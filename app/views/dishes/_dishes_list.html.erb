
		<table summery="dish lists">
		<tr>	
			<th><u>name</u></th>
			<th>&nbsp;</th>
			<th><u>taste</u></th>	
			<th><u>culture</u></th>
			<th>&nbsp;<u>mealpart</u></th>
			<th>&nbsp;</th>
		</tr>
<% dishes.each do |dish| %>
	<tr>
		<td><%= link_to(dish.name, {:action =>'show',:id => dish.id}, :class => 'action')%></td>
		<td>&nbsp;&nbsp;<%= link_to(image_tag(dish.image_url.to_s, :size => '50x50'), {:action =>'show',:id => dish.id}, :class => 'action')%></td>
		<td>&nbsp;<%= dish.taste %></td>
		<td>&nbsp;&nbsp;<%= dish.culture %></td>
		<td>&nbsp;&nbsp;<%= dish.mealpart %></td>
		<%if user_id=session[:user_id]%>				
			<td>&nbsp;<%=link_to("delete recipe",{:action =>'delete',:id=>dish.id, state: private}, :class => 'action' )%></td>
		<%end%>
			
	</tr>
	<br />
	<hd />
<%end%>
</table>
